{
  "name": "fond-of-spryker/customer-b2b",
  "type": "library",
  "description": "FondOf B2b Customer module",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "spryker/customer": "^7.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.12",
    "codeception/codeception": "^2.4",
    "codeception/mockery-module": "^0.2",
    "mikey179/vfsStream": "^1.6",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    }
  }
}
